{
  "node": true,
  "browser": true,
  "qunit": true,
  "globals": {
    "HTMLDocument": false,
    "define": false
  },
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "strict": true,
  "maxparams": 5,
  "maxdepth": 5,
  "maxstatements": 30,
  "maxcomplexity": 10
}
